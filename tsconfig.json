{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module":"es2015",
    "target": "es6",
    "lib": [ "esnext", "dom" ],
    "jsx": "preserve",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": false,
    "allowJs": true,
    "strictPropertyInitialization": false,
    "strictNullChecks": false
  }
}